{
   "name": "csakis",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "react": "^16.8.3",
      "react-dom": "^16.8.3",
      "react-scripts": "2.1.5"
   },
   "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "sw": "sw-precache --root=build --config=sw-precache-config.js --verbose",
      "add-domain": "echo \"csakis.net\" > build/CNAME",
      "postbuild": "react-snap && npm run sw && npm run add-domain"
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
   ],
   "devDependencies": {
      "react-snap": "^1.23.0",
      "sw-precache": "^5.2.1"
   },
   "reactSnap": {
      "skipThirdPartyRequests": true,
      "inlineCss": true
   }
}
